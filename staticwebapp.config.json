{
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": [
      "/images/*",
      "/css/*",
      "*.css",
      "*.js",
      "*.ico",
      "*.jpg",
      "*.png",
      "*.svg"
    ]
  },
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "mimeTypes": {
    ".json": "text/json",
    ".css": "text/css",
    ".js": "text/javascript",
    ".html": "text/html"
  },
  "globalHeaders": {
    "cache-control": "must-revalidate, max-age=86400",
    "Content-Security-Policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' https: data:;"
  }
}
